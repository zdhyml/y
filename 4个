#include <stdio.h>
void main()
{
	char str[] = "3+4";
	int a;
	a = str[0] + str[2] - '0' - '0';
	printf("%d\n",a);
}

#include <stdio.h>
#include <string.h>
void main()
{
	char str[] = "1+2+2+1+2+5+4-1-3+4-8";
	int a = strlen(str), sum = str[0] - '0', j;
	for (j = 0;j < a;j++)
	{
		if (str[j + 1] == '+' || str[j + 1] == '-')
		{
			if (str[j + 1] == '+')
				sum = sum + str[j + 2] - '0';
			else
				sum = sum - (str[j + 2] - '0');

		}
	}
	printf("%d", sum);

}

#include <stdio.h>
#include <string.h>
void main()
{
	char str[] = "2*2/4*1/1*2*3/2";
	int a = strlen(str), sum = str[0] - '0', j;
	for (j = 0;j < a;j++)
	{
		if (str[j + 1] == '*' || str[j + 1] == '/')
		{
			if (str[j + 1] == '*')
				sum = sum *(str[j + 2] - '0');
			else
				sum = sum /(str[j + 2] - '0');

		}
	}
	printf("%d", sum);

}

#include <stdio.h>
#include <string.h>
void main()
{
	char str[] = "2+2*3+2/2-1";
	int a = strlen(str), sum = str[0] - '0', j;
	for (j = 0;j < a;j++)
	{
		if (str[j + 1] == '+' || str[j + 1] == '-')
		{
			if (str[j + 1] == '+')
				sum = sum + str[j + 2] - '0';
			else
				sum = sum - (str[j + 2] - '0');

		}
		else
		{
			if (str[j + 1] == '*')
				sum = sum *(str[j + 2] - '0');
			else
				sum = sum / (str[j + 2] - '0');
		}
	}
	printf("%d", sum);

}
